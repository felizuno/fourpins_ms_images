{
  "manifest_version": 2,
  "name": "M&S Image Extractor",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://maasandstacks.com/*",
        "*://www.maasandstacks.com/*"
      ],
      "js": ["jquery.js", "main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "fourpins-logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}